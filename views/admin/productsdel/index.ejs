
<% include ../header %>    

<body>

    <div id="wrapper">

        <!-- Navigation -->
<% include ../menu %>            
        

        <div id="page-wrapper">

            <div class="container-fluid">

              
                
                
                <div class="row top-heading">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                            Products  <small>  </small>
                        </h1>


                        <ol class="breadcrumb">
                            <li class="active">
                                <i class="fa fa-dashboard"></i> Products
                            </li>
                        </ol>

                    </div>



                </div>
              

                <div class="row">
                    <div class="col-lg-12 panel">
                        <a href="/admin/productCreate"> <span class="btn-info btn pull-right"> Create1 </span> </a>
                    </div>
                    
                     <div class="col-lg-12 panel">
                    
                    <br>    
                     <div class="table-responsive">
                            <table class="table table-bordered table-hover table-striped">
                                <thead>
                                    <tr>
                                        <th>S.No</th>
                                        <th>Product Title</th>
                                        <th> Product Price </th>
                                        <th> Product Discount </th>
                                        <th> Action </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for( var i in data ) { %>
                                    <tr>
                                        <td> <%- i %> </td>
                                        <td><%- data[i].product_title %></td>
                                        <td> <%- data[i].product_price %> </td>
                                        <td> <%- data[i].product_discount %> </td>
                                        <td> <a href="/admin/products/<%- data[i]._id %>">  View / Edit </a> </td>
                                    </tr>
                               <% }    %>
                                    
                                </tbody>
                            </table>
                        </div>
                    
                     </div>
                </div>
                <!-- /.row -->

               
                <!-- /.row -->

                <!-- /.row -->

            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

   <% include ../footer %> 

</body>

</html>
