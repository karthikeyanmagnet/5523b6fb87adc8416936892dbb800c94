
<% include header %>    

<body>

    <div id="wrapper">

        <!-- Navigation -->
<% include menu %>            
        

        <div id="page-wrapper">

            <div class="container-fluid">

                <!-- Page Heading -->
                
                
                 <div class="row top-heading">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                            Users List  <small> Statistics Overview </small>
                        </h1>


                        <ol class="breadcrumb">
                            <li class="active">
                                <i class="fa fa-dashboard"></i> User List
                            </li>
                        </ol>

                    </div>



                </div>
              

                <div class="row">
                    
                    
                    <div class="table-responsive">
                            <table class="table table-bordered table-hover table-striped">
                                <thead>
                                    <tr>
                                        <th>S.No</th>
                                        <th>Name</th>
                                        <th> Email </th>
                                        <th> Mobile Number </th>
                                        <th> Action </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for( var i in data ) { %>
                                    <tr>
                                        <td> <%- i %> </td>
                                        <td><%- data[i].first_name+" "+data[i].last_name %></td>
                                        <td> <%- data[i].email %> </td>
                                        <td> <%- data[i].mobilenumber %> </td>
                                        <td> <a href="/admin/users/<%- data[i]._id %>">  View </a> </td>
                                    </tr>
                               <% }    %>
                                    
                                </tbody>
                            </table>
                        </div>
                    
                </div>
                <!-- /.row -->

               
                <!-- /.row -->

                <!-- /.row -->

            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

   <% include footer %> 

</body>

</html>
